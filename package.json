{
  "name": "audio-suite-pro",
  "version": "1.0.0",
  "description": "A modern web-based audio transcription and text processing application with OpenAI integration",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server . -p 8080 -o",
    "lint": "eslint script.js",
    "lint:fix": "eslint script.js --fix",
    "format": "prettier --write *.js *.html *.css *.md",
    "format:check": "prettier --check *.js *.html *.css *.md",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run lint && npm run format:check && npm run test",
    "dev": "npx http-server . -p 8080",
    "prepare": "husky install"
  },
  "keywords": [
    "audio",
    "transcription",
    "openai",
    "speech-to-text",
    "web-app",
    "javascript",
    "multilingual"
  ],
  "author": "ChrisDCG",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisDCG/Lokal_neu.git"
  },
  "bugs": {
    "url": "https://github.com/ChrisDCG/Lokal_neu/issues"
  },
  "homepage": "https://github.com/ChrisDCG/Lokal_neu#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "prettier": "^3.0.0",
    "http-server": "^14.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}